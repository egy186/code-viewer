    "bower": "bower install && mkdirp chrome/content/lib/highlightjs && ncp bower_components/highlightjs/highlight.pack.min.js chrome/content/lib/highlightjs/highlight.pack.min.js && ncp bower_components/highlightjs/styles chrome/content/lib/highlightjs/styles",
    "build": "npm run bower && npm run xpi",
    "clean": "rimraf chrome/content/lib",
    "lint": "eslint chrome/content/js data index.js",
    "start": "jpm run",
    "xpi": "jpm xpi"
  },
